    "            style=\"\"\"\n",
    "                background-color: #F0EFEF;\n",
    "                border: 0.5px solid black;\n",
    "                border-radius: 3px;\n",
    "                box-shadow: 2px;\n",
    "            \"\"\",\n",
    "            max_width=800,\n",
    "        )\n",
    "\n",
    "        folium.GeoJson(\n",
    "            data=mapping(bus_intersection_bus_stops_4326),\n",
    "            name=f'Overlap Bus Stops for Bus {bus_service} - MRT {mrt_line}',\n",
